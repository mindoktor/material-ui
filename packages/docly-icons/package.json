{
  "name": "@docly/guide-icons",
  "main": "src/index.js",
  "scripts": {
    "preconvert": "rimraf src/icons",
    "convert": "svgr -d src/icons src/images && yarn convert:toc",
    "convert:toc": "cd ../../ && babel-node --config-file ./babel.config.js packages/docly-icons/convert.js --out packages/docly-icons/src/icons",
    "prebuild": "rimraf build",
    "build:es2015": "cross-env NODE_ENV=production babel --config-file ../../babel.config.js ./src/icons --out-dir ./build",
    "build": "yarn convert && yarn build:es2015"
  },
  "private": true,
  "author": "Docly Healthcare AB",
  "version": "0.0.1",
  "sideEffects": false,
  "engines": {
    "node": ">=6.0.0"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.1.2",
    "@svgr/cli": "^4.0.0",
    "fs-extra": "^7.0.0",
    "yargs": "^12.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
